{
  "name": "@lukvvonstrom/aws-infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "nx exec -- cdk",
    "synth": "nx exec -- cdk synthesize",
    "bootstrap": "nx exec -- cdk bootstrap",
    "deploy": "nx exec -- cdk deploy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "18.15.11",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "aws-cdk": "2.73.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "nx": "15.9.2",
    "@nrwl/nx-cloud": "latest"
  },
  "dependencies": {
    "aws-cdk-lib": "2.80.0",
    "constructs": "^10.1.303",
    "source-map-support": "^0.5.21"
  },
  "nx": {
    "targets": {
      "synth": {
        "outputs": [
          "{projectRoot}/cdk.out"
        ]
      },
      "bootstrap": {
        "outputs": [
          "{projectRoot}/cdk.out"
        ]
      },
      "deploy": {
        "outputs": [
          "{projectRoot}/cdk.out"
        ]
      }
    }
  }
}
